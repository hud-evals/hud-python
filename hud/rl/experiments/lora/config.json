{
  "base_model": "Qwen/Qwen2.5-VL-3B-Instruct",
  "processor": {
    "min_pixels": 50176,
    "max_pixels": 200704,
    "trust_remote_code": true
  },
  "num_gpus": 1,
  "training_steps": 10,
  "buffer": {
    "shuffle_dataset": false,
    "buffer_steps": 4,
    "select_strategy": "recent",
    "require_images": false
  },
  "batch_size": 32,
  "mini_batch_size": 2,
  "group_size": 4,
  "rewards": {
    "scale_rewards": "group",
    "leave_one_out": false
  },
  "training": {
    "model": {
      "attn_implementation": "flash_attention_2",
      "trust_remote_code": true,
      "use_liger": true,
      "freeze_vision_tower": true,

      "enable_lora": true,
      "lora_r": 16,
      "lora_alpha": 32,
      "lora_dropout": 0.05
    },
    "dp_replicate": 1,
    "dp_shard": 1,
    "save_last_n": 1,
    "loss": {
      "apply_length_norm": true,
      "importance_sampling_level": "token",
      "kl_beta": 0.0,
      "clip_ratio": 10.0
    },
    "optimizer": {
      "lr": 1e-4,
      "use_8bit_optimizer": true,
      "adam_betas": [
        0.9,
        0.999
      ],
      "adam_eps": 1e-08
    },
    "max_grad_norm": 1.0
  },
  "client": {
    "base_url": "http://127.0.0.1:8000/v1",
    "api_key": "EMPTY",
    "request_timeout": 45.0
  },
  "actor": {
    "verbose": false,
    "max_steps_per_episode": 5,
    "max_parallel_episodes": 48,
    "temperature": 0.7,
    "max_new_tokens": 1024,
    "force_tool_choice": false,
    "episode_timeout_sec": 600
  },
  "vllm": {
    "host": "127.0.0.1",
    "port": 8000,
    "tensor_parallel_size": 1,
    "enable_auto_tool_choice": true,
    "tool_call_parser": "hermes",
    "chat_template": null,
    "uvicorn_log_level": null,
    "enable_log_requests": false,
    "gpu_memory_utilization": 0.9,
    "extra_vllm_args": []
  },
  "job_name": "lora-rl",
  "stats_interval": 1,
  "verbose": false,
  "seed": 1234,
  "output_dir": "./outputs"
}
